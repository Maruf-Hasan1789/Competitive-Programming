ll compute_hash1(string const& s)
{
    const int p=31;
    const int m=1e9+9;
    ll hash_val=0;
    ll p_pow=1;
    for(int i=0;i<s.size();i++)
    {
        hash_val=(hash_val+(s[i]-'a'+1)*p_pow)%m;
        p_pow=(p_pow*p)%m;
    }
    return hash_val;
}

ll compute_hash2(string const& s)
{
    const int p=37;
    const int m=1e9+21;
     ll hash_val=0;
    ll p_pow=1;
    for(int i=0;i<s.size();i++)
    {
        hash_val=(hash_val+(s[i]-'a'+1)*p_pow)%m;
        p_pow=(p_pow*p)%m;
    }
    return hash_val;
}

